{
  "name": "ssex",
  "version": "1.6.17",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "concurrently -k \"npm run start --workspace=@ssex/server\" \"npm run dev --workspace=@ssex/client\"",
    "dev": "concurrently -k \"npm run dev --workspace=@ssex/server\" \"npm run dev --workspace=@ssex/client\"",
    "build": "npm run build --workspace=@ssex/client",
    "seed": "node scripts/migrate_data.js",
    "server": "npm run start --workspace=@ssex/server",
    "client": "npm run dev --workspace=@ssex/client"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "yargs": "^17.7.2"
  }
}